<div class="modal" id="modalVerPendientes" tabindex="-1" role="dia" aria-hidden="true">
    <div class="mb10"></div>
    <div class="modal-lg col-lg-offset-3">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Maestros Pendientes por Aprobar</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="table-responsive col-lg-12">
                        <table class="table" id="tablaMaestroPend">
                            
                        </table>
                    </div><!-- table-responsive -->
